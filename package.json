    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.7",
    "@nestjs/platform-ws": "^8.4.7",
    "@nestjs/websockets": "^8.4.7",
    "aios": "^1.0.1",
    "aws-sdk": "^2.1167.0",
    "code-complexity": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.2",
    "ngx-socket-io": "^4.3.0",
    "reflect-metadata": "^0.1.13",
